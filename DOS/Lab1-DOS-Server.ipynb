    "import socket\n",
    "s=socket.socket()\n",
    "print(\"server created\")\n",
    "s.bind(('localhost', 9992))\n",
    "s.listen(3)\n",
    "print('waiting for connection')\n",
    "c,addr=s.accept()\n",
    "print(c,addr)\n",
    "recvdmsg=c.recv(1024)\n",
    "print(recvdmsg)\n",
    "c.close()\n",
    "\n"
