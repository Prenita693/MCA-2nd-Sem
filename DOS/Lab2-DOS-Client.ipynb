    "import xmlrpc.client\n",
    "\n",
    "# Create server proxy\n",
    "proxy = xmlrpc.client.ServerProxy(\"http://localhost:8000/RPC2\")\n",
    "\n",
    "# Remote function call\n",
    "result = proxy.add(15, 5)\n",
    "print(f\"Result of remote addition: {result}\")\n"
